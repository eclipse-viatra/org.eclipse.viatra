<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.viatra.query.patternlanguage.emf.annotation">
      <annotation
            name="Optional">
      </annotation>
      <annotation
            name="Param1">
         <annotationparameter
               mandatory="false"
               multiple="false"
               name="p1"
               type="string">
         </annotationparameter>
      </annotation>
      <annotation
            name="Param2">
         <annotationparameter
               mandatory="true"
               multiple="false"
               name="p1">
         </annotationparameter>
         <annotationparameter
               mandatory="false"
               multiple="false"
               name="p2">
         </annotationparameter>
      </annotation>
   </extension>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated typetest -->
      <package
            uri="http://www.eclipse.org/viatra/query/patternlanguage/emf/test"
            class="org.eclipse.viatra.query.patternlanguage.emf.tests.pltest.PltestPackage"
            genModel="model/typetest.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.viatra.query.patternlanguage.emf.purewhitelist">
      <pure-element
            fully-qualified-name="org.eclipse.viatra.query.patternlanguage.emf.tests.DummyClass"
            type="class">
      </pure-element>
      <pure-element
            fully-qualified-name="package.Class.method()"
            type="method">
      </pure-element>
   </extension>

</plugin>
