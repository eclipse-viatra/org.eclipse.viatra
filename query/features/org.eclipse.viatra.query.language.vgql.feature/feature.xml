This program and the accompanying materials are made available
under the terms of the Eclipse Public License v1.0 which accompanies
this distribution, and is available at http://www.eclipse.org/legal/epl-v10.html
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="VIATRA" url="http://download.eclipse.org/viatra/updates/release/"/>
   </url>

   <requires>
      <import feature="org.eclipse.viatra.query.sdk.feature" version="2.1.1" match="equivalent"/>
      <import feature="org.eclipse.jdt"/>
      <import feature="org.eclipse.pde"/>
      <import feature="org.eclipse.viatra.integration.gmf.feature" version="2.1.1" match="equivalent"/>
   </requires>

   <plugin
         id="org.eclipse.viatra.query.patternlanguage.emf.sirius"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.viatra.query.patternlanguage.metamodel"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.viatra.query.patternlanguage.metamodel.edit"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.viatra.query.patternlanguage.metamodel.validation"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
