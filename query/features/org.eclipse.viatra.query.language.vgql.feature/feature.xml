<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.viatra.query.language.vgql.feature"
      label="VIATRA Graphical Query Editor (Incubation)"
      version="0.29.1.qualifier"
      provider-name="Eclipse VIATRA Project"
      license-feature="org.eclipse.license"
      license-feature-version="2.0.2">

   <description>
      A graphical graph query language for VIATRA.
   </description>

   <copyright>
      Copyright (c) 2018 IncQuery Labs Ltd. and others.
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="VIATRA" url="http://download.eclipse.org/viatra/updates/release/"/>
   </url>

   <requires>
      <import feature="org.eclipse.viatra.query.sdk.feature" version="2.9.0" match="equivalent"/>
      <import feature="org.eclipse.jdt"/>
      <import feature="org.eclipse.pde"/>
      <import feature="org.eclipse.viatra.integration.gmf.feature" version="2.9.0" match="equivalent"/>
      <import feature="org.eclipse.sirius.properties.feature" version="7.0.0" match="compatible"/>
      <import feature="org.eclipse.sirius.runtime" version="7.0.0" match="compatible"/>
      <import feature="org.eclipse.sirius.runtime.aql" version="7.0.0" match="compatible"/>
   </requires>

   <plugin
         id="org.eclipse.viatra.query.patternlanguage.emf.sirius"
         version="0.0.0"/>

   <plugin
         id="org.eclipse.viatra.query.patternlanguage.metamodel"
         version="0.0.0"/>

   <plugin
         id="org.eclipse.viatra.query.patternlanguage.metamodel.edit"
         version="0.0.0"/>

   <plugin
         id="org.eclipse.viatra.query.patternlanguage.metamodel.validation"
         version="0.0.0"/>

</feature>
