<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.viatra.query.sdk.extensions.feature"
      label="VIATRA Query and Transformation SDK Extensions (Incubation)"
      version="0.26.0.qualifier"
      provider-name="Eclipse VIATRA Project"
      license-feature="org.eclipse.license"
      license-feature-version="2.0.2">

   <description>
      Additional features extending the capabilities of the VIATRA Query and Transformation SDK. The features here are considered experimental, and can be changed or removed in future versions without prior notice.
   </description>

   <copyright>
      Copyright (c) 2010,2018 Budapest University of Technology and Economics, IncQuery Labs Ltd. and others.
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="VIATRA" url="http://download.eclipse.org/viatra/updates/release/"/>
   </url>

   <includes
         id="org.eclipse.viatra.transformation.debug.feature"
         version="0.0.0"/>

   <includes
         id="org.eclipse.viatra.addon.viewers.runtime.zest.feature"
         version="2.6.0.qualifier"
         name="VIATRA Viewers Runtime - GEF4 Zest Support"/>

   <requires>
      <import feature="org.eclipse.viatra.query.sdk.feature" version="2.6.0" match="equivalent"/>
      <import feature="org.eclipse.jdt"/>
      <import feature="org.eclipse.pde"/>
      <import feature="org.eclipse.viatra.addon.viewers.runtime.zest.feature" version="2.6.0" match="compatible"/>
      <import feature="org.eclipse.gef.graph" version="5.1.0" match="compatible"/>
      <import feature="org.eclipse.gef.layout" version="5.0.0" match="compatible"/>
      <import feature="org.eclipse.gef.zest.fx.jface" version="5.1.1" match="compatible"/>
   </requires>

   <plugin
         id="org.eclipse.viatra.query.tooling.debug"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.viatra.query.tooling.ui.retevis"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.viatra.query.tooling.localsearch.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.viatra.addon.viewers.tooling.ui.zest"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
