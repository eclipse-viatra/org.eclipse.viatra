<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.viatra.query.tooling.ui.visualization.feature"
      label="VIATRA Query Visualization (Incubation)"
      version="0.17.3.qualifier"
      provider-name="Eclipse VIATRA Project"
      license-feature="org.eclipse.license"
      license-feature-version="0.0.0">

   <license url="%licenseURL">
      %license
   </license>

   <description>
      VIATRA Query Visualization Features. Requires the GEF 5 version of Zest.
   </description>

   <url>
      <update label="VIATRA" url="http://download.eclipse.org/viatra/updates/release/"/>
   </url>

   <requires>
      <import feature="org.eclipse.viatra.query.sdk.feature" version="1.6.0" match="compatible"/>
      <import feature="org.eclipse.viatra.addon.viewers.runtime.zest.feature" version="0.16.0" match="compatible"/>
      <import feature="org.eclipse.gef.graph" version="5.0.0" match="compatible"/>
      <import feature="org.eclipse.gef.layout" version="5.0.0" match="compatible"/>
      <import feature="org.eclipse.gef.zest.fx.jface" version="5.0.0" match="compatible"/>
   </requires>

   <plugin
         id="org.eclipse.viatra.query.tooling.ui.retevis"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.viatra.query.tooling.localsearch.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
