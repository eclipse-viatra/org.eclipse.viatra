<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.viatra.addon.databinding.runtime.feature"
      label="VIATRA Data Binding Integration"
      version="2.7.0.qualifier"
      provider-name="Eclipse VIATRA Project"
      license-feature="org.eclipse.license"
      license-feature-version="2.0.2">

   <description>
      Generic JFace Databinding support for VIATRA Query result sets.
   </description>

   <copyright>
      Copyright (c) 2010,2018 Budapest University of Technology and Economics, IncQuery Labs Ltd. and others.
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="VIATRA" url="http://download.eclipse.org/viatra/updates/release/"/>
   </url>

   <requires>
      <import feature="org.eclipse.emf.databinding"/>
      <import feature="org.eclipse.viatra.query.runtime.feature" version="2.7.0" match="equivalent"/>
      <import feature="org.eclipse.viatra.transformation.runtime.emf.feature" version="2.7.0" match="equivalent"/>
   </requires>

   <plugin
         id="org.eclipse.viatra.addon.databinding.runtime"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
